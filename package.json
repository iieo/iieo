{
  "name": "nextjs-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write .",
    "types": "tsc --noEmit",
    "format:check": "prettier --check .",
    "lint": "next lint",
    "db:generate": "drizzle-kit generate:pg --config=./src/db/drizzle.config.json",
    "db:migrate": "dotenv -e .env tsx ./src/db/migrate.ts"
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "0.7.3",
    "drizzle-orm": "0.29.3",
    "next": "14.1.0",
    "next-auth": "4.24.5",
    "postgres": "3.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "three": "0.168.0",
    "zod": "3.22.4"
  },
  "packageManager": "pnpm@9.1.3",
  "devDependencies": {
    "@titanom/prettier-config": "2.0.0",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@types/three": "0.168.0",
    "autoprefixer": "10.0.1",
    "dotenv-cli": "7.3.0",
    "drizzle-kit": "0.20.13",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "postcss": "8.4.33",
    "tailwindcss": "3.3.0",
    "tsx": "4.7.0",
    "typescript": "5.3.3"
  }
}
